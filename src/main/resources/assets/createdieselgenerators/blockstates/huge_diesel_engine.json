      "apply": {
        "model": "create:block/fluid_pipe/rim/north"
      },
      "when": {
        "north": "true"
      }
    },
    {
      "apply": {
        "model": "create:block/fluid_pipe/rim/down"
      },
      "when": {
        "down": "true"
      }
    },
    {
      "apply": {
        "model": "create:block/fluid_pipe/rim/up"
      },
