      },
      "when": {
        "facing": "west"
      }
    },
    {
      "apply": {
        "model": "createdieselgenerators:block/huge_diesel_engine/block",
        "y": 90
      },
      "when": {
        "facing": "east"
      }
    },
    {
      "apply": {
        "model": "createdieselgenerators:block/huge_diesel_engine/block",
